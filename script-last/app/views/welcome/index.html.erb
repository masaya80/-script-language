<h1>Welcome#index</h1>

<% if user_signed_in? %>
  <p>メールアドレス <%= current_user.email %></p>
  <p>氏名 <%= current_user.name %></p>
  <%= link_to "マイページへ", current_user %>
  <%= link_to 'Logout', destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to 'Login', new_user_session_path %>
<% end %>